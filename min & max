MAX_SIZE = 100
arr = []
size = int(input("Enter size of the array: "))
print("Enter elements in the array: ")
for i in range(size):
    arr.append(int(input()))
max = arr[0]
min = arr[0]
for i in range(1, size):
    if arr[i] > max:
        max = arr[i]
    if arr[i] < min:
        min = arr[i]
print("Maximum element =", max)
print("Minimum element =", min)
